{
    "builds": [
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "command": "bash build.sh"
        }
      },
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  